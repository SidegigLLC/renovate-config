{
  "extends": [
    "config:js-app",
    "group:linters",
    "group:unitTest",
    "npm:unpublishSafe",
    ":rebaseStalePrs",
    ":label(renovate)",
    ":timezone(America/New_York)",
    ":automergeTesters",
    ":automergeLinters",
    ":automergeTypes",
    ":automergeMinor",
    ":automergePatch",
    ":automergeBranch",
    ":enableVulnerabilityAlerts"
  ],
  "prCreation": "not-pending",
  "prConcurrentLimit": 2,
  "major": {
    "dependencyDashboardApproval": true
  },
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": ["before 2am on the first day of the month"]
  },
  "schedule": [
    "before 3am and after 8pm on Tuesday",
    "before 3am and after 8pm on Wednesday",
    "before 3am and after 8pm on Thursday"
  ],
  "minimumReleaseAge": 3
}
